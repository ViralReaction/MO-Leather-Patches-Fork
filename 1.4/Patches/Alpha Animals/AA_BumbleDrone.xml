<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
		
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<ThingDef ParentName="DankPyon_HideBaseMedium">
							<defName>DankPyon_Hide_Bumbledrone</defName>
							<label>medium bumbledrone hide</label>
							<graphicData>
								<color>(126,112,116)</color>
							</graphicData>
							<statBases>
								<MarketValue>75.6</MarketValue>
							</statBases>
							<stackLimit>4</stackLimit>
							<butcherProducts Inherit="False">
								<Leather_Chitin>28</Leather_Chitin>
							</butcherProducts>
							<stuffProps>
								<color>(126,112,116)</color>
							</stuffProps>
						</ThingDef>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef [defName="AA_Bumbledrone"]/lifeStages/li[3]</xpath>

					<value>
