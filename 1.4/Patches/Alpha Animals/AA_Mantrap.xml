<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
		
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<ThingDef ParentName="DankPyon_HideBaseHuge">
							<defName>DankPyon_Hide_Mantrap</defName>
							<label>huge mantrap hide</label>
							<graphicData>
								<color>(189,161,116)</color>
							</graphicData>
							<statBases>
								<MarketValue>205.2</MarketValue>
							</statBases>
							<butcherProducts Inherit="False">
								<Leather_Cactus>60</Leather_Cactus>
							</butcherProducts>
							<stuffProps>
								<color>(189,161,116)</color>
							</stuffProps>
						</ThingDef>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef [defName="AA_Mantrap"]/lifeStages/li[3]</xpath>

					<value>
						<butcherBodyPart>
							<bodyPartGroup>HeadAttackTool</bodyPartGroup>
							<thing>DankPyon_Hide_Mantrap</thing>
							<allowFemale>true</allowFemale>
						</butcherBodyPart>
					</value>
				</li>

			</operations>
		</match>
		
	</Operation>
	
</Patch>