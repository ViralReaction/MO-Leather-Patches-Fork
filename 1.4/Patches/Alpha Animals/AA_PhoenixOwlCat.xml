<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
		
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<ThingDef ParentName="DankPyon_HideBaseTiny">
							<defName>DankPyon_Hide_PhoneixOwlcat</defName>
							<label>tiny phoenix owlcat hide</label>
							<graphicData>
								<color>(126,112,116)</color>
							</graphicData>
							<statBases>
								<MarketValue>39.69</MarketValue>
							</statBases>
							<stackLimit>6</stackLimit>
							<butcherProducts Inherit="False">
								<Leather_Light>21</Leather_Light>
							</butcherProducts>
							<stuffProps>
								<color>(126,112,116)</color>
							</stuffProps>
						</ThingDef>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef [defName="AA_PhoenixOwlcat"]/lifeStages/li[3]</xpath>

					<value>
						<butcherBodyPart>
							<bodyPartGroup>HeadAttackTool</bodyPartGroup>
							<thing>DankPyon_Hide_PhoneixOwlcat</thing>
							<allowFemale>true</allowFemale>
						</butcherBodyPart>
					</value>
				</li>

			</operations>
		</match>
		
	</Operation>
	
</Patch>